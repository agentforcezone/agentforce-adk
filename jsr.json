{
    "name": "@agentforce/adk",
    "version": "0.4.0",
    "description": "AgentForce Agent Development Kit - A powerful framework for building AI agents and servers with TypeScript, featuring chainable APIs, multiple LLM provider support, and built-in server capabilities.",
    "license": "AgentForce License",
    "exports": "./lib/mod.ts",
    "publish": {
        "include": [
            "lib/**/*",
            "README.md",
            "LICENSE"
        ],
        "exclude": [
            "tests/**/*",
            "docs/**/*",
            "examples/**/*",
            "templates/**/*",
            "_/**/*",
            "**/*.test.ts",
            "**/*.test.js",
            "*.md",
            "!README.md",
            "tsconfig.json",
            "bun.lock*",
            "Dockerfile*",
            "docker-compose.yml",
            "podman-compose.yml",
            "k8s-deployment.yaml",
            "kind-config.yaml",
            "debug-agent.ts",
            "run-podman.sh",
            ".npmignore"
        ]
    }
}